import sys
def div(a,b):
    assert a>0, " a should be greater than 0"
    try:
        c=a/b
    except ZeroDivisionError:
        print("b cannot be equal to 0")
    else:
        return c
val1=int(input("enter the value of number 1 "))
val2=int(input("enter the value of number 2 "))
val3=div(val1,val2)
print(val1,"/",val2,"=",val3)